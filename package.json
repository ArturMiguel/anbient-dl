{
    "name": "anbient-dl",
    "version": "1.1.9",
    "description": "CLI para download de animes do Anbient/Zippyshare.",
    "main": "dist/index.js",
    "scripts": {
        "init": "rm -rf node_modules/ && rm -f package-lock.json && npm i && npm run build",
        "build": "rm -rf dist/ && tsc"
    },
    "bin": {
        "dl": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ArturMiguel/anbient-dl.git"
    },
    "keywords": [
        "Web-scraper",
        "Anime",
        "Downloader",
        "Anbient"
    ],
    "author": "Artur Miguel",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ArturMiguel/anbient-dl/issues"
    },
    "homepage": "https://github.com/ArturMiguel/anbient-dl#readme",
    "dependencies": {
        "colors": "^1.4.0",
        "commander": "^4.1.0",
        "inquirer": "^7.3.3",
        "ora": "^4.0.3",
        "progress": "^2.0.3",
        "puppeteer": "^2.1.1",
        "request": "^2.88.0",
        "update-notifier": "^4.1.0"
    },
    "devDependencies": {
        "@types/inquirer": "^7.3.1",
        "@types/progress": "^2.0.3",
        "@types/puppeteer": "^3.0.1",
        "@types/request": "^2.48.5",
        "@types/update-notifier": "^4.1.1",
        "typescript": "^4.0.2"
    }
